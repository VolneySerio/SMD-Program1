void setup() {
  size(400, 400);
}

// line:
void draw() {
  stroke(#ff0000);
  line(200, 200, 235, 235);
}

// rect:
void draw() {
  fill(#96ecf9);
  stroke(#96ecf9);
  rect(0, 0, 100, 100);
}

// ellipse: 
void draw() {
  fill(#9600f9);
  stroke(#c9c9c9);
  ellipse(200, 200, 100, 100);
}

// triangle:
void draw() {
  stroke(#ffffff);
  triangle(100, 100, 50, 200, 150, 200);
}